{
    "extends": "solhint:recommended",
    "rules": {
        "compiler-version": [
            "error",
            ">=0.4.0 <0.9.0"
        ],
        "func-visibility": [
            "warn",
            {
                "ignoreConstructors": false
            }
        ],
        "func-name-mixedcase": "warn"
    }
}